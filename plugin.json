{
  "name": "custom.remote.python.auditor",
  "version": "0.1.5",
  "type": "python",
  "requiredAgentVersion": "1.101.0",
  "entity": "CUSTOM_DEVICE",
  "metricGroup": "tech.Auditor",
  "technologies": ["Auditor"],
  "source": {
    "package": "audit_activegate_plugin",
    "className": "AuditPluginRemote",
    "install_requires": ["requests>=2.6.0"],
    "activation": "Remote"
  },
  "metrics": [
    {
      "entity": "CUSTOM_DEVICE",
      "timeseries": {
        "key": "rtt",
        "unit": "MilliSecond",
        "displayname": "Round-trip time"
      }
    }
  ],
  "properties": [
    {"key": "domain", "type": "String"},
    {"key": "tenant", "type": "String"},
		{"key": "apiToken", "type": "Password"},
    {"key": "pollingInterval", "type": "Integer"}
  ],
  "configUI": {
    "displayName": "Auditor",
    "properties": [
      {"key" : "domain", "displayName": "Domain of the Dynatrace API", "displayOrder": 1, "displayHint": "For example: usg925.dynatrace-managed.com"},
      {"key" : "tenant", "displayName": "Tenant of the Dynatrace API", "displayOrder": 2, "displayHint": "For example: c28e9814-d46d-4b11-8ba0-6f76708e384e"},
			{"key" : "apiToken", "displayName": "API Token with access to V1 metrics and V2 Audit Logs", "displayOrder": 3, "displayHint": "For example: ABllUTJYQwKKQRWSpPIva"},
      {"key" : "pollingInterval", "displayName": "Polling frequency (in minutes)", "displayOrder": 4, "displayHint": "For example: 5"}
    ]
  }
}